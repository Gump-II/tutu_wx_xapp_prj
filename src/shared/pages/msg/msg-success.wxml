<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">操作成功</view>
            <view class="weui-msg__desc">
                {{contentMsg}}
            </view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap="pageBack">确定</button>
                <button class="weui-btn" type="default" bindtap="pageBack">返回</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <navigator url="" class="weui-footer__link">途徒自由行</navigator>
                </view>
                <view class="weui-footer__text">Copyright © 2016-2017 tutufree.com</view>
            </view>
        </view>

    </view>
</view>